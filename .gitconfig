[user]
	name = Steve Martin
	email = steve.martin@fundingcircle.com
[alias]
  co = checkout
  b = branch
  a = add
  ls = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate
  ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
[push]
	default = current
[core]
	editor ='vi -u NONE'
	excludesfile = ~/.gitignore
[rerere]
	enabled = true
